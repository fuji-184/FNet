
<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>net9.0</TargetFramework>
    <LangVersion>latest</LangVersion>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>FNet</PackageId>
    <Version>0.0.1</Version>
    <Authors>Fuji</Authors>
    <Description>Simple C# HTTP 1.1 library. Experimental</Description>
    <RepositoryUrl>https://github.com/fuji-184/FNet</RepositoryUrl>
    <PackageTags>http;socket;async</PackageTags>
    </PropertyGroup>

</Project>
